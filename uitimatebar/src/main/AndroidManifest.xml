<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="okay.test.uitimatebar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ColorBarActivity"/>
        <activity android:name=".DrawerActivity"/>
        <activity android:name=".HintBarActivity1"/>
        <activity android:name=".HintBarActivity2"/>
        <activity android:name=".TransparentBarActivity1"/>
        <activity android:name=".TransparentBarActivity2"/>
        <activity android:name=".ImmersionBarActivity"/>
    </application>

</manifest>