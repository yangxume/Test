<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="okay.test.timeline">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TimeLineBasicActivity"
            android:label="@string/basic_timeline"/>
        <activity android:name=".TimeLineInRecyclerViewActivity"
            android:label="@string/horizontal_vertical_timeline"/>
        <activity android:name=".TimeLineTestActivity"
            android:label="@string/vertiacl_timeline"/>

    </application>

</manifest>